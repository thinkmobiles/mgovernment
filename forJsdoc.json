{
  "tags": {
    "allowUnknownTags": true
  },

  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "Mgoverment",
    "footer": "",
    "copyright": "Thinkmobiles Copyright © 2014-2015 The contributors to the JSDoc3 and DocStrap projects.",
    "navType": "inline",
    "theme": "cosmo",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "highlightTutorialCode": true,
    "syntaxTheme": "dark"
  },

  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },

  "source": {
    "include": ["handlers/", "routes/", "models/" ],
    "includePattern": ".+\\.js(doc)?$"
  },

  "opts": {
    "destination": "./doc/",
    "template": "node_modules/grunt-jsdoc/node_modules/ink-docstrap/template"
  }

}